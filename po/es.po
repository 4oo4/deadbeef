#: ../main.c:129
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Uso: deadbeef [opciones] [--] [archivo(s)]\n"

#: ../main.c:130
#, c-format
msgid "Options:\n"
msgstr "Opciones:\n"

#: ../main.c:131
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  o  -h      Mostrar la ayuda (este mensaje) y salir\n"

#: ../main.c:132
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Salir del reproductor\n"

#: ../main.c:133
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Mostrar la versión y salir\n"

#: ../main.c:134
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Iniciar la reproducción\n"

#: ../main.c:135
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Parar la reproducción\n"

#: ../main.c:136
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausar la reproducción\n"

#: ../main.c:137
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Pausar o reanudar\n"

#: ../main.c:138
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Iniciar la reproducción si está detenida, pausar en caso contrario\n"

#: ../main.c:139
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Pista siguiente de la lista de reproducción\n"

#: ../main.c:140
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Pista anterior de la lista de reproducción \n"

#: ../main.c:141
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Canción al azar de la lista de reproducción\n"

#: ../main.c:142
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Añadir archivo(s) a la lista de reproducción\n"

#: ../main.c:143
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui COMPLEMENTO  Indica qué complemento de IGU usar; GTK2 es el predeterminado\n"

#: ../main.c:144
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Envía por la salida estándar el nombre formateado de la pista\n"

#: ../main.c:145
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-sintaxis:  a: Artista, t: Título, b: Álbum,\n                      l: Duración, n: Pista, y: Año, c: Comentario,\n                      r: Copyright, e: Tiempo restante\n"

#: ../main.c:148
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "                      ejemplo: --nowplaying «%%a - %%t» mostraría «artista - título»\n"

#: ../main.c:149
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      más información en %s\n"

#: ../main.c:150
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTA:  --nowplaying está considerada obsoleta.\n"

#: ../main.c:151
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "   --nowplaying-tf FMT  Envía por la salida estándar el nombre formateado de la pista,\n                      usando el formato de título nuevo\n"

#: ../main.c:152
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "                      Sintaxis FMT: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:153
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "                      ejemplo: --nowplaying-tf «%%artist%% - %%title%%» mostraría «artista - título»\n"

#: ../main.c:154
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "--volume NUM       Muestra o configura el volumen.\n"

#: ../main.c:155
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "                      El parámetro NUM puede especificarse en porcentaje (valor absoluto o incremento/reducción)\n"

#: ../main.c:156
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                     o en dB [-50, 0] (si es con sufijo).\n"

#: ../main.c:157
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "                     Ejemplos: --volume 80, --volume +10, --volume -5 o --volume -20dB\n"

#: ../main.c:158
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../main.c:159
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../main.c:160
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

