#: ../main.c:129
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Käyttö: deadbeef [asetukset] [--] [tiedosto(t)]\n"

#: ../main.c:130
#, c-format
msgid "Options:\n"
msgstr "Valitsimet:\n"

#: ../main.c:131
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  tai  -h    Näytä ohje (tämä viesti) ja poistu\n"

#: ../main.c:132
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Sulje soitin\n"

#: ../main.c:133
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Näytä versiotiedot ja poistu\n"

#: ../main.c:134
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Aloita toisto\n"

#: ../main.c:135
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Pysäytä toisto\n"

#: ../main.c:136
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Keskeytä toisto\n"

#: ../main.c:137
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Vaihda keskeytyksen ja toiston välillä\n"

#: ../main.c:138
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Aloita toisto, jos pysäytetty, muuten vaihda keskeytyksen ja toiston välillä\n"

#: ../main.c:139
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Seuraava kappale soittolistalla\n"

#: ../main.c:140
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Edellinen kappale soittolistalla\n"

#: ../main.c:141
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Satunnainen kappale soittolistalla\n"

#: ../main.c:142
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Lisää tiedosto(t) olemassa olevaan soittolistaan\n"

#: ../main.c:143
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "--gui PLUGIN Kertoo mitä graafisen käyttöliittymän liitännäistä käytetään, oletus on \"GTK2\"\n"

#: ../main.c:144
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Tulosta muotoiltu raidan nimi stdoutiin\n"

#: ../main.c:145
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-syntaksi: [a]rtist, [t]itle, al[b]um,\n                      [l]ength, track[n]umber, [y]ear, [c]omment,\n                      copy[r]ight, [e]lapsed\n"

#: ../main.c:148
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "esimerkki: --nowplaying \"1%%a - 1%%t\" :n pitäisi tulostaa \"artisti - kappalenimi\"\n"

#: ../main.c:149
#, c-format
msgid "                      for more info, see %s\n"
msgstr "lisätietoja saadaksesi, katso 1%s\n"

#: ../main.c:150
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "HUOM: --nowplaying on vanhentunut.\n"

#: ../main.c:151
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "--nowplaying-tf FMT  Tulosta muotoiltu kappaleennimi stdout virtaan käyttämällä uutta otsikon muotoilua\n"

#: ../main.c:152
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr "FMT syntaksi: http://github.com/DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:153
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "esimerkki: --nowplaying-tf \"%% artist 1%% - %% title 1%%\" pitäisi tulostaa \"artist - title\"\n"

#: ../main.c:154
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "--volume [NUM]  Tulosta tai aseta deadbeef:in äänen voimakkuus.\n"

#: ../main.c:155
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr "NUM parametri voidaan määritellä prosentteina (absoluuttinen arvo tai lisäyksenä/vähennyksenä)\n"

#: ../main.c:156
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "tai dB:issä [-50, 0] (jos yksikön kanssa).\n"

#: ../main.c:157
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr "Esimerkit: --volume 80, --volume +10, --volume -5 tai --volume -20dB\n"

#: ../main.c:158
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../main.c:159
#, c-format
