#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "--nowplaying \"%%a - %%t\" 會顯示 \"藝人 - 歌名\"\n"

#: ../main.c:149
#, c-format
msgid "                      for more info, see %s\n"
msgstr "更多資訊請見 %s\n"

#: ../main.c:150
